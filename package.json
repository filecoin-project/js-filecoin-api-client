{
  "name": "filecoin-api-client",
  "version": "0.3.0",
  "description": "API client for Filecoin",
  "main": "src/index.js",
  "browser": {
    "./src/lib/form-data.js": "./src/lib/form-data.browser.js"
  },
  "scripts": {
    "build": "webpack",
    "test": "npm run test:unit",
    "test:unit": "ava --verbose test/unit/**/*.test.js test/unit/**/**/*.test.js",
    "test:e2e": "node test/e2e/_warn.js && ava --verbose test/e2e/**/*.test.js",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "lint": "standard"
  },
  "author": "Alan Shaw",
  "license": "(Apache-2.0 OR MIT)",
  "dependencies": {
    "async-iterator-to-stream": "^1.1.0",
    "explain-error": "^1.0.4",
    "form-data": "^2.5.0",
    "is-buffer": "^2.0.3",
    "iterable-ndjson": "^1.1.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "abort-controller": "^3.0.0",
    "ava": "^2.2.0",
    "cids": "^0.7.1",
    "ipfs-unixfs": "^0.1.16",
    "multiaddr": "^6.1.0",
    "nyc": "^14.1.1",
    "standard": "^13.0.2",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "failWithoutAssertions": false
  },
  "files": [
    "src",
    "dist"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filecoin-project/js-filecoin-api-client.git"
  },
  "keywords": [
    "Filecoin",
    "FIL",
    "API",
    "HTTP",
    "REST"
  ],
  "bugs": {
    "url": "https://github.com/filecoin-project/js-filecoin-api-client/issues"
  },
  "homepage": "https://github.com/filecoin-project/js-filecoin-api-client#readme"
}
